def min(values):          #define the function by def    #min() and max()is built-in function
    smallest = None
    for value in values:
        if smallest is None or value <smallest:
            smallest = value
    return smallest

y=5
while y>0:                #condition it will take 5 integer values
  values = input('>')     #taking input from user
  y=y-1
  x=min(values)           #call the function min ()
print(x)
